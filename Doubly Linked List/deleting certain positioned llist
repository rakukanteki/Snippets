//DELETING A CERTAIN NODE
struct node* delposition(struct node* head,int position)
{
    struct node* temp, *temp2;
    temp=head;
    temp2=NULL;
    if(position==1)
    {
        head=delfirst(head);
    }
    else
    {
        while(position>1)
        {
            temp=temp->next;
            position--;
        }
    }
    if(temp->next==NULL)
    {
        head=dellast(head);
    }
    else
    {
        temp2=temp->next;
        temp2->next=temp->next;
        temp->next->prev=temp2;
        free(temp);
        temp=NULL;
    }
    return head;
}
