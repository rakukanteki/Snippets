//FUNCTION FOR ADDING A NODE AT THE END OF A LIST

void at_the_end(struct node* head, value)
{
    struct node* ptr,temp;
    ptr=head;
    //temp contains the node to be added at the end
    temp = (struct node*)malloc(sizeof(struct node));
    temp->data=value;
    temp->link=NULL;

    while(ptr->link != NULL)
    {
        ptr=ptr->link;
    }
    ptr->link=temp;

}
